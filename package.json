    "bugs": {
        "url": "https://github.com/interglobalmedia/basic-audio-visual-slider/issues"
    },
    "homepage": "https://github.com/interglobalmedia/basic-audio-visual-slider#readme",
    "dependencies": {
        "gh-pages": "^2.2.0",
        "node-sass": "^4.14.0"
    }
}